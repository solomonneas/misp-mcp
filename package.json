{
  "name": "misp-mcp",
  "version": "1.0.0",
  "description": "MCP server for MISP threat intelligence platform - IOC lookups, event management, correlation discovery, and intelligence enrichment",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "misp-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit"
  },
  "author": "Solomon Neas <me@solomonneas.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
